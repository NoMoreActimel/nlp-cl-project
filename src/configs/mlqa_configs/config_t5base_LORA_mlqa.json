  "n_gpu": 1,
  "preprocessing": {},
  "model": {
    "type": "T5LoRA",
    "args": {
      "t5_config": {
        "model_name": "t5-base",
        "cache_dir": "cache/",
        "max_length": 512
      },
      "lora_config": {
        "rank": 4, 
        "alpha": 16, 
        "dropout_p": 0.1,
        "target_layers": ["q", "k", "v", "o", "wi", "wo"]
      }
    }
