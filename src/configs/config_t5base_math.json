{
  "name": "T5_CL_summarization",
  "n_gpu": 1,
  "preprocessing": {},
  "model": {
      "type": "T5forSummarization",
      "args": {
        "model_name": "t5-base",
        "cache_dir": "cache/",
        "max_length": 256
      }
  },
  "data": {
    "train": {
      "batch_size": 8,
      "num_workers": 5,
      "max_length": 256,
      "tokenizer_name": ["T5Tokenizer", "t5-base"],
      "use_mixed_dataset": true,
      "mixed_dataset": {
        "type": "MixedSequentialDataset",
        "args": {
          "sequential_mixing_rate": 0.1
        }
      },
      "datasets": [
        {
          "type": "MathDataset",
          "args": {
            "path": "deepmind/math_dataset/arithmetic__add_or_sub",
            "split": "train",
            "as_supervised": true
          }
        },
        {
          "type": "MathDataset",
          "args": {
            "path": "deepmind/math_dataset/arithmetic__add_or_sub_in_base",
            "split": "train",
            "as_supervised": true
          }
        },
        {
          "type": "MathDataset",
          "args": {
            "path": "deepmind/math_dataset/arithmetic__add_sub_multiple",
            "split": "train",
            "as_supervised": true
          }
        },
        {
          "type": "MathDataset",
          "args": {
            "path": "deepmind/math_dataset/arithmetic__div",
            "split": "train",
            "as_supervised": true
          }
        },
        {
          "type": "MathDataset",
          "args": {
            "path": "deepmind/math_dataset/arithmetic__mixed",
            "split": "train",
            "as_supervised": true
          }
        },
        {
          "type": "MathDataset",
          "args": {
            "path": "deepmind/math_dataset/arithmetic__mul",
            "split": "train",
            "as_supervised": true
          }
        },
        {
          "type": "MathDataset",
          "args": {
            "path": "deepmind/math_dataset/arithmetic__mul_div_multiple",
            "split": "train",
            "as_supervised": true
          }
        },
        {
          "type": "MathDataset",
          "args": {
            "path": "deepmind/math_dataset/arithmetic__nearest_integer_root",
            "split": "train",
            "as_supervised": true
          }
        },
        {
          "type": "MathDataset",
          "args": {
            "path": "deepmind/math_dataset/arithmetic__simplify_surd",
            "split": "train",
            "as_supervised": true
          }
        }
      ]
    },
    "val_add_or_sub": {
      "batch_size": 16,
      "num_workers": 5,
      "max_length": 256,
      "tokenizer_name": ["T5Tokenizer", "t5-base"],
      "use_mixed_dataset": false,
      "datasets": [
        {
          "type": "MathDataset",
          "args": {
            "path": "deepmind/math_dataset/arithmetic__add_or_sub",
            "split": "test",
            "as_supervised": true
          }
        }
      ]
    },
    "val_add_or_sub_in_base": {
      "batch_size": 16,
      "num_workers": 5,
      "max_length": 256,
      "tokenizer_name": ["T5Tokenizer", "t5-base"],
      "use_mixed_dataset": false,
      "datasets": [
        {
          "type": "MathDataset",
          "args": {
            "path": "deepmind/math_dataset/arithmetic__add_or_sub_in_base",
            "split": "test",
            "as_supervised": true
          }
        }
      ]
    },
    "val_add_sub_multiple": {
      "batch_size": 16,
      "num_workers": 5,
      "max_length": 256,
      "tokenizer_name": ["T5Tokenizer", "t5-base"],
      "use_mixed_dataset": false,
      "datasets": [
        {
          "type": "MathDataset",
          "args": {
            "path": "deepmind/math_dataset/arithmetic__add_sub_multiple",
            "split": "test",
            "as_supervised": true
          }
        }
      ]
    },
    "val_div": {
      "batch_size": 16,
      "num_workers": 5,
      "max_length": 256,
      "tokenizer_name": ["T5Tokenizer", "t5-base"],
      "use_mixed_dataset": false,
      "datasets": [
        {
          "type": "MathDataset",
          "args": {
            "path": "deepmind/math_dataset/arithmetic__div",
            "split": "test",
            "as_supervised": true
          }
        }
      ]
    },
    "val_mixed": {
      "batch_size": 16,
      "num_workers": 5,
      "max_length": 256,
      "tokenizer_name": ["T5Tokenizer", "t5-base"],
      "use_mixed_dataset": false,
      "datasets": [
        {
          "type": "MathDataset",
          "args": {
            "path": "deepmind/math_dataset/arithmetic__mixed",
            "split": "test",
            "as_supervised": true
          }
        }
      ]
    },
    "val_mul": {
      "batch_size": 16,
      "num_workers": 5,
      "max_length": 256,
      "tokenizer_name": ["T5Tokenizer", "t5-base"],
      "use_mixed_dataset": false,
      "datasets": [
        {
          "type": "MathDataset",
          "args": {
            "path": "deepmind/math_dataset/arithmetic__mul",
            "split": "test",
            "as_supervised": true
          }
        }
      ]
    },
    "val_mul_div_multiple": {
      "batch_size": 16,
      "num_workers": 5,
      "max_length": 256,
      "tokenizer_name": ["T5Tokenizer", "t5-base"],
      "use_mixed_dataset": false,
      "datasets": [
        {
          "type": "MathDataset",
          "args": {
            "path": "deepmind/math_dataset/arithmetic__mul_div_multiple",
            "split": "test",
            "as_supervised": true
          }
        }
      ]
    },
    "val_nearest_integer_root": {
      "batch_size": 16,
      "num_workers": 5,
      "max_length": 256,
      "tokenizer_name": ["T5Tokenizer", "t5-base"],
      "use_mixed_dataset": false,
      "datasets": [
        {
          "type": "MathDataset",
          "args": {
            "path": "deepmind/math_dataset/arithmetic__nearest_integer_root",
            "split": "test",
            "as_supervised": true
          }
        }
      ]
    },
    "val_simplify_surd": {
      "batch_size": 16,
      "num_workers": 5,
      "max_length": 256,
      "tokenizer_name": ["T5Tokenizer", "t5-base"],
      "use_mixed_dataset": false,
      "datasets": [
        {
          "type": "MathDataset",
          "args": {
            "path": "deepmind/math_dataset/arithmetic__simplify_surd",
            "split": "test",
            "as_supervised": true
          }
        }
      ]
    }
  },
  "optimizer": {
    "type": "AdamW",
    "args": {
      "lr": 5e-1,
      "betas": [0.9, 0.95],
      "weight_decay": 1e-5
    }
  },
  "metrics": [
    {
      "type": "AverageRougeMetric",
      "args": {
        "name": "AverageRouge"
      }
    }
  ],
  "lr_scheduler": {
    "type": "ExponentialLR",
    "args": {
      "gamma": 1.0
    }
  },
  "loss": {
    "type": "CrossEntropyLoss",
    "args": {}
  },
  "trainer": {
    "epochs": 60,
    "eval_frequency": 10,
    "save_dir": "saved/",
    "save_period": 60,
    "verbosity": 2,
    "monitor": "min loss",
    "visualize": "wandb",
    "wandb_project": "nlp-cl-project",
    "wandb_run_name": "T5b Mix=0.1 w/o grad clip",
    "grad_norm_clip": 100.0
  }
}